{
    "highlight_selected_line": true,
    "show_margin": false,
    "show_invisibles": true,
    "show_indent_guides": true,
    "tab_multiline_completion": true,
    "show_help_tooltip_on_idle": true,
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "auto_append_newline": true,
    "soft_wrap_r_files": true,
    "save_before_sourcing": false,
    "syntax_color_console": true,
    "highlight_r_function_calls": true,
    "highlight_code_chunks": false,
    "font_size_points": 14,
    "editor_theme": "Monokai",
    "default_encoding": "UTF-8",
    "default_project_location": "~/Desktop",
    "restore_source_documents": false,
    "check_for_updates": false,
    "document_author": "T.N.",
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "always_save_history": false,
    "reuse_sessions_for_project_links": true,
    "load_workspace": false,
    "initial_working_directory": "~/Desktop",
    "windows_terminal_shell": "win-cmd",
    "posix_terminal_shell": "bash",
    "jobs_tab_visibility": "shown",
    "rmd_viewer_type": "pane",
    "rmd_chunk_output_inline": false,
    "scroll_past_end_of_document": true,
    "rainbow_parentheses": true,
    "source_with_echo": true,
    "remove_history_duplicates": true,
    "restore_last_project": false,
    "code_completion_characters": 2,
    "style_diagnostics": true,
    "show_diagnostics_cpp": false,
    "show_diagnostics_other": false,
    "panes": {
        "quadrants": [
            "Console",
            "TabSet1",
            "Source",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Viewer"
        ],
        "hiddenTabSet": [
            "Tutorial"
        ],
        "console_left_on_top": true,
        "console_right_on_top": true,
        "additional_source_columns": 0
    }
}
